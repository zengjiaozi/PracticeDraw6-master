<?xml version="1.0" encoding="utf-8"?>
<com.hencoder.hencoderpracticedraw6.practice.practice08.Practice08ObjectAnimatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2B2B2B">

    <com.hencoder.hencoderpracticedraw6.practice.practice08.Practice08ObjectAnimatorView
        android:id="@+id/objectAnimatorView8"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerInParent="true"/>

    <Button
        android:id="@+id/animateBt8"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="8dp"
        android:text="@string/animate"
        android:textSize="16sp"/>

    <EditText
        android:id="@+id/editText8"
        android:layout_width="50dp"
        android:layout_height="48dp"
        android:layout_alignParentBottom="true"
        android:background="@color/colorAccent"
        android:inputType="number"/>


</com.hencoder.hencoderpracticedraw6.practice.practice08.Practice08ObjectAnimatorLayout>